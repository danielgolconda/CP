#include <iostream>
#include <bits/stdc++.h>
#define lld long long int
using namespace std;
lld a[150];
lld k;
lld c(lld n,lld k)
{
    if(n == 0)
    {
        return 1;
    }
    if(n < 0)
    {
        return 0;
    }
    if(k <= 0 && n>=1)
    {
        return 0;
    }
    return c(n-a[k-1],k)+c(n,k-1);
}
int main() 
{
	lld sum;
	cin >> sum >> k;
	for(int i=0;i<k;i++)
	{
	    cin >> a[i];
	}
	lld x=c(sum,k);
	cout<<x<<endl;
	return 0;
}
